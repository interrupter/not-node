{
  "name": "not-node",
  "version": "3.4.36",
  "description": "node complimentary part for client side notFramework.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require mocha-suppress-logs --reporter spec  --timeout 12000",
    "lint": "./node_modules/.bin/eslint ./src/**/**/**/**.js --fix",
    "pretest": "./node_modules/.bin/eslint ./src/**/**/**/**.js",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "js-watch": "watch 'npm run cover' ./src ./test --interval=5",
    "watch:build:cover:dev": "npm-run-all --parallel js-watch",
    "cover": "nyc npm test"
  },
  "bin": {
    "not-node": "./bin/not-node.js",
    "not-builder": "./bin/not-builder.js"
  },
  "engines": {
    "node": ">12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interrupter/not-node.git"
  },
  "keywords": [
    "notFramework",
    "node"
  ],
  "author": "interrupter <levoe.milo@mail.ru> (http://github.com/interrupter)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interrupter/not-node/issues"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongodb-session": "^3.1.1",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "deep-diff": "*",
    "deepmerge": "^4.2.2",
    "ejs": "^3.1.6",
    "escape-string-regexp": "*",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.2",
    "fs-extra": "*",
    "helmet": "^4.6.0",
    "lower-case": "*",
    "method-override": "^3.0.0",
    "mongoose": "*",
    "mongoose-validator": "*",
    "nconf": "*",
    "not-config": "^0.1.3",
    "not-error": "*",
    "not-filter": "*",
    "not-inform": "^0.0.27",
    "not-locale": "*",
    "not-log": "^0.0.20",
    "not-monitor": "^0.0.13",
    "not-path": "*",
    "redis": "^3.1.2",
    "rmdir": "^1.2.0",
    "serve-static": "*",
    "simple-git": "*",
    "yargs": "*"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "babel-eslint": "^10.1.0",
    "chai": "*",
    "chai-as-promised": "*",
    "eslint": "^7.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-sonarjs": "^0.10.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.7",
    "mocha": "*",
    "mocha-suppress-logs": "^0.3.1",
    "mongodb-memory-server": "^7.5.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "retire": "^3.0.3"
  },
  "homepage": "https://github.com/interrupter/not-node#readme",
  "nyc": {
    "include": [
      "index.js",
      "src/**/**/**.js"
    ],
    "exclude": [
      "tests/**/**/**/**.js"
    ],
    "reporter": [
      "html"
    ],
    "check-coverage": false,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
