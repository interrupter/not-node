{
  "name": "not-node",
  "version": "3.4.4",
  "description": "node complimentary part for client side notFramework.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec  --timeout 12000",
    "lint": "./node_modules/.bin/eslint ./src/**/**.js --fix",
    "pretest": "./node_modules/.bin/eslint ./src/**/**",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "cover": "nyc npm test"
  },
  "bin": {
    "not-node": "./bin/not-node.js",
    "not-builder": "./bin/not-builder.js"
  },
  "engines": {
    "node": ">12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interrupter/not-node.git"
  },
  "keywords": [
    "notFramework",
    "node"
  ],
  "author": "interrupter <levoe.milo@mail.ru> (http://github.com/interrupter)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interrupter/not-node/issues"
  },
  "dependencies": {
    "deep-diff": "*",
    "deepmerge": "^4.2.2",
    "ejs": "^3.1.6",
    "escape-string-regexp": "*",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "*",
    "lower-case": "*",
    "mongoose": "*",
    "mongoose-validator": "*",
    "nconf": "^0.11.2",
    "not-error": "^0.0.38",
    "not-filter": "*",
    "not-locale": "*",
    "not-log": "*",
    "not-path": "*",
    "not-ws": "*",
    "rmdir": "^1.0.0",
    "serve-static": "^1.14.1",
    "simple-git": "*",
    "yargs": "*"
  },
  "devDependencies": {
    "chai": "*",
    "chai-as-promised": "*",
    "eslint": "*",
    "extend": "^3.0.2",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.6",
    "mocha": "*",
    "mongodb-memory-server": "^6.9.6",
    "nyc": "^15.1.0",
    "retire": "^2.2.4"
  },
  "homepage": "https://github.com/interrupter/not-node#readme",
  "nyc": {
    "include": [
      "index.js",
      "src/**/**/**.js"
    ],
    "exclude": [
      "tests/**/**/**/**.js"
    ],
    "reporter": [
      "html"
    ],
    "check-coverage": false,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
