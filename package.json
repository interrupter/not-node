{
	"name": "not-node",
	"version": "6.5.37",
	"description": "node complimentary part for client side notFramework.",
	"main": "index.js",
	"scripts": {
		"test": "mocha --exit --require mocha-suppress-logs --reporter spec  --timeout 12000",
		"lint": "eslint ./src --fix",
		"pretest": "eslint ./src",
		"docs": "jsdoc -c jsdoc.json",
		"js-watch": "watch 'npm run cover' ./src ./test --interval=5",
		"watch:build:cover:dev": "npm-run-all --parallel js-watch",
		"cover": "nyc npm test",
		"clear:playground": "rm -rf ./playground",
		"prepare": "husky"
	},
	"bin": {
		"not-node": "bin/not-node.js",
		"not-cli": "bin/not-cli.mjs",
		"not-builder": "bin/not-builder.js"
	},
	"engines": {
		"node": ">14.9"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-node.git"
	},
	"keywords": [
		"notFramework",
		"node"
	],
	"author": "interrupter <levoe.milo@mail.ru> (http://github.com/interrupter)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-node/issues"
	},
	"dependencies": {
		"ajv": "^8.17.1",
		"body-parser": "^1.20.2",
		"commander": "^12.1.0",
		"compression": "^1.7.4",
		"connect-mongodb-session": "^5.0.0",
		"connect-redis": "^7.1.1",
		"cors": "^2.8.5",
		"deep-diff": "*",
		"deepmerge": "^4.3.1",
		"ejs": "^3.1.10",
		"escape-string-regexp": "*",
		"express": "^4.19.2",
		"express-fileupload": "^1.5.1",
		"express-session": "^1.18.0",
		"fs-extra": "*",
		"generate-password": "^1.7.1",
		"helmet": "^7.1.0",
		"inquirer": "^12.0.0",
		"inquirer-autocomplete-prompt": "^3.0.1",
		"jsonwebtoken": "^9.0.2",
		"method-override": "^3.0.0",
		"mongoose-validator": "*",
		"nconf": "*",
		"not-config": "*",
		"not-filter": "^0.3.15",
		"not-inform": "*",
		"not-locale": "^0.0.22",
		"not-log": "*",
		"not-monitor": "*",
		"not-path": "*",
		"rate-limiter-flexible": "^5.0.3",
		"redis": "^4.7.0",
		"redlock": "^5.0.0-beta.2",
		"rfdc": "^1.4.1",
		"rimraf": "^6.0.1",
		"serve-static": "*",
		"simple-git": "*",
		"validator": "*",
		"xss": "*",
		"yargs": "*"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"chai": "*",
		"chai-as-promised": "*",
		"eslint": "^9.9.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-sonarjs": "^2.0.2",
		"husky": "^9.1.5",
		"ink-docstrap": "^1.3.2",
		"ioredis": "^5.4.1",
		"jsdoc": "^4.0.3",
		"mocha": "*",
		"mocha-suppress-logs": "^0.5.1",
		"mock-require": "^3.0.3",
		"mongodb-memory-server": "^10.0.0",
		"mongoose": "^8.6.0",
		"not-error": "^0.3.0",
		"not-validation": "^0.0.9",
		"npm-run-all": "^4.1.5",
		"nyc": "^17.0.0",
		"retire": "^5.2.2"
	},
	"homepage": "https://github.com/interrupter/not-node#readme",
	"nyc": {
		"include": [
			"index.js",
			"src/**/**/**.js"
		],
		"exclude": [
			"tests/**/**/**/**.js"
		],
		"reporter": [
			"html"
		],
		"check-coverage": false,
		"per-file": true,
		"lines": 95,
		"statements": 95,
		"functions": 95,
		"branches": 95,
		"watermarks": {
			"lines": [
				80,
				95
			],
			"functions": [
				80,
				95
			],
			"branches": [
				80,
				95
			],
			"statements": [
				80,
				95
			]
		}
	}
}